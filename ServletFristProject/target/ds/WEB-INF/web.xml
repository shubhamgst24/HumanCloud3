
 <web-app xml="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
          http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">
    <display-name>Webapp1</display-name>
    <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    
    <servlet>
    <servlet-name>LDSServlet</servlet-name>
    <servlet-class>com.hc.lds.servlet.LDSServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>LDSServlet</servlet-name>
    <url-pattern>/hc/*</url-pattern>
    </servlet-mapping>

	<servlet>
    <servlet-name>MenuServlet</servlet-name>
    <servlet-class>com.hc.lds.servlet.MenuServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>MenuServlet</servlet-name>
    <url-pattern>/menu/*</url-pattern>
    </servlet-mapping>
   
	<servlet>
    <servlet-name>DataStructureServlet</servlet-name>
    <servlet-class>com.hc.lds.servlet.DataStructureServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>DataStructureServlet</servlet-name>
    <url-pattern>/d/*</url-pattern>
    </servlet-mapping>

    
</web-app>
